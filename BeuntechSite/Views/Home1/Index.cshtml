    <div class="bg-overlay">
        <div class="cta-wrapper">
            <h3 class="cta-lead h-alt wow fadeIn" data-wow-delay=".1s" data-wow-duration="1s">Join our newsletter</h3>
            <form class="mc-ajax mc-form subscribe-form" _lpchecked="1">
